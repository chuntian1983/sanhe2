function ShowVoucher(bid,aid,vid)
{
    window.showModalDialog("AccountManage/LookVoucher.aspx?bid="+bid+"&aid="+aid+"&id="+vid+"&g="+(new Date()).getTime(),"","dialogWidth=767px;dialogHeight=385px;center=yes;");
