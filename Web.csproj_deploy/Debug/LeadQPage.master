var isExpand=true;
function ToggleMenu()
{
    if(isExpand)
    {
        isExpand=false;
        document.getElementById("LeftMenu").style.display="none";
        document.getElementById("SplitButton").src="Images/split_right.gif";
    }
    else
    {
